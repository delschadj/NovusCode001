{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["warn", { "args": "none" }],
    "import/no-unresolved": "off",  // Temporarily disable this rule
    "import/named": "off",
    "no-console": "warn",
    "react/display-name": "off",    // Disable missing display name errors
    "react/no-children-prop": "off" // Disable errors for passing children as props
  }
}
